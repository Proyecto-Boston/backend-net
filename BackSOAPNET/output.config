<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ServicePortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:1802/app" binding="basicHttpBinding"
                bindingConfiguration="ServicePortBinding" contract="Service"
                name="ServicePort" />
        </client>
    </system.serviceModel>
</configuration>